class Vehicle {
  String brand;
  int year;

  Vehicle(this.brand, this.year);
}
class Car extends Vehicle {
  String model;
  Car(String brand, int year, this.model) : super(brand, year);
 void displayInfo() {
    print("Brand: $brand");
    print("Year: $year");
    print("Model: $model");
  }
}
void main() {
  Car car1 = Car('Toyota', 2022, 'Fortuner');
  car1.displayInfo();
}
